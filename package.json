{
  "name": "min-seed",
  "version": "1.0.0",
  "description": "A minimal Node web app starter with Express, Webpack, and Gulp.",
  "main": "./build/server.js",
  "scripts": {
    "build": "yarn clean && gulp build && yarn webpack",
    "clean": "rm -rf ./build",
    "lint": "./node_modules/.bin/eslint ./src",
    "start:dev": "yarn build && NODE_ENV=development gulp serve",
    "start:prod": "NODE_ENV=production node ./build/server/server.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-core/register --exit --colors ./test/**/*.js",
    "webpack": "./node_modules/.bin/webpack",
    "webpack:watch": "./node_modules/.bin/webpack --watch --env=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grantschulte/min-seed.git"
  },
  "keywords": [
    "node",
    "express",
    "webpack",
    "gulp",
    "seed",
    "starter"
  ],
  "author": "Grant J. Schulte <schulte.grant@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "copy-webpack-plugin": "4.2.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint-loader": "1.9.0",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "gulp": "3.9.1",
    "gulp-babel": "7.0.0",
    "gulp-dest": "0.2.3",
    "gulp-livereload": "3.8.1",
    "gulp-nodemon": "2.2.1",
    "imagemin-webpack-plugin": "1.5.2",
    "morgan": "1.9.0",
    "node-sass": "4.7.1",
    "postcss-clean": "1.1.0",
    "postcss-loader": "2.0.8",
    "pug": "2.0.0-rc.4",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "1.1.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "eslint": "4.11.0",
    "gulp-eslint": "4.0.0",
    "mocha": "4.0.1"
  },
  "engines": {
    "node": "8.9.0"
  }
}
